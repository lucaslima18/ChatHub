#Quando a selagem de notas fiscais é obrigatória? ---> (obrigatorio)
~[obrigatorio]
    ~[quando] a ~[selagem] ~[da] ~[nota] ~[fiscal] ~[deve_ser] @[obrigatorio]
    a ~[selagem] ~[de] ~[nota] ~[fiscal] ~[deve_ser] @[obrigatorio]
    ~[selagem] ~[de] ~[nota] ~[fiscal] ~[deve_ser] @[obrigatorio]

~[nota]
    nota
    nfce
    notinha
    nota
    nf

~[fiscal]
    fiscal
    fiscais

~[selagem]
    selo 
    selagem
    selamento
    aplicar selo
    aplico selo
    inserção selo
    inserir selo

@[obrigatorio]
    obrigatorio
    obrigatoria
    obrigatório
    obrigatória
